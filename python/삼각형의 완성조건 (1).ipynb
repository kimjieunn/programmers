   "source": [
    "## 프로그래머스\n",
    "### 삼각형의 완성조건 (1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "bc15ed21",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(sides):\n",
    "    if (sum(sides) - max(sides)) <= max(sides):\n",
    "        answer = 2\n",
    "    else:\n",
    "        answer = 1   \n",
    "    return answer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "cd193f3c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2\n",
      "2\n",
      "1\n"
     ]
    }
   ],
   "source": [
    "print(solution([1, 2, 3]))\n",
    "print(solution([3, 6, 2]))\n",
    "print(solution([199, 72, 222]))"
   ]
  }
