    "## 프로그래머스\n",
    "### 문자 반복 출력하기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "bc15ed21",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(my_string, n):\n",
    "    answer = []\n",
    "    \n",
    "    for i in range(len(my_string)):\n",
    "        for j in range(n):\n",
    "            answer.append(my_string[i])\n",
    "    return ''.join(answer)"
   ]
  },
