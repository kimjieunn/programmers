   "source": [
    "## 프로그래머스\n",
    "### 짝수의 합"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "bc15ed21",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    for i in range(n + 1):\n",
    "        if (i % 2) == 0:\n",
    "            answer += i            \n",
    "    return answer"
   ]
  },
