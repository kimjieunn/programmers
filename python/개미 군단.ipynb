   "source": [
    "def solution(hp):\n",
    "    answer = 0\n",
    "    a = hp // 5\n",
    "    b = (hp-(5*a)) // 3\n",
    "    c = (hp-(5*a)-(3*b)) // 1\n",
    "    return a+b+c"
   ]
