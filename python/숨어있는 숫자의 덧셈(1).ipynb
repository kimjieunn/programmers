   "source": [
    "## 프로그래머스\n",
    "### 숨어있는 숫자의 덧셈(1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "bc15ed21",
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "\n",
    "def solution(my_string):\n",
    "    answer = re.findall(r'\\d', my_string)\n",
    "    answer = [int(i) for i in answer]\n",
    "    return sum(answer)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "cd193f3c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1\n",
      "0\n"
     ]
    }
   ],
   "source": [
    "print(solution(3, 2))\n",
    "print(solution(10, 5))"
   ]
  }
